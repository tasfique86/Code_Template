bool cmp(pair<int,int>a, pair<int,int> b)
{
    if(max(a.F,a.S)==max(b.F,b.S))
    {
        if(min(a.F,a.S) < min(b.F,b.S ))
            return true;
        else
            return false;

    }
    else
    {
        if(max(a.F,a.S)>max(b.F,b.S))
            return false;
        else
           return true;
    }
}

