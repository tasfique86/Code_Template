const ll N=1e6;
vector<bool> isPrime(N+2,1);
//vector<int> lp(N+2,0),hp(N+2,0);
vl primess;
//int has[N];
void SievePrime()
{
    isPrime[0]=isPrime[1]=false;
    for(ll i=2; i<=N; i++)


    {
        if(isPrime[i]== true)
        {
            primess.pb(i);

            //lp[i]=hp[i]=i;
            for(ll j=i*i; j<=N; j+=i)
            {
                isPrime[j]=false;
                // hp[j]=i;
                /* if(lp[j]==0)
                 {
                   lp[j]=i;
                 }*/
            }
        }
    }

}



ll binaryExpoIter( ll a, ll b  )
{
    ll res=1;
    while(b)
    {
        if(b&1)
        {
            res=res*a;
        }
        a =a*a;
        b=b>>1;

    }

    return res;
}



