        while(in<n)
        {
            vec[i].pb(v[in]);
            in+=d;
        }
        srt(vec[i]);
      }
      ll szz=vec[0].size();
      vl tv;
      for(int i=0;i<szz;i++)
      {
        for(int j=0;j<d;j++)
