//ll dp[10004]={-1};

ll fun(ll amount,vl &v){
if(amount==0)
  return 0;

ll ans=INT_MAX;
 for(auto coin: v){
  if(amount-coin>=0)
      ans=min(ans,fun(amount-coin,v)+1);
 }
 return ans;

}

int ok(ll in,ll tg, ll m)
{
    ll sm=(in*(in+1))/2;
    sm-=m;
  //  cout<<in<<" "<<tg<<" "<<sm<<endl;
    if(sm<=tg)
        return 1;
    return 0;

}


